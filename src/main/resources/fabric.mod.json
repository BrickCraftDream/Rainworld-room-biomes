{
  "schemaVersion": 1,
  "id": "rainworld",
  "version": "${version}",
  "name": "Rainworld MC Biomes",
  "description": "Adds all rooms and their screens from Rain World, Downpour and Watcher to Minecraft as biomes. Also includes a tool for placing those biomes, inventory palettization based on the current biome and multiple graphical adjustments like removing biome border randomization and disabling per-side block shading.",
  "authors": [
    "BrickCraftDream"
  ],
  "contact": {
    "repo": "https://github.com/BrickCraftDream/Rainworld-room-biomes"
  },
  "license": "CC0-1.0",
  "icon": "assets/rainworld/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.brickcraftdream.rainworldmc_biomes.client.Rainworld_MC_BiomesClient"
    ],
    "main": [
      "net.brickcraftdream.rainworldmc_biomes.Rainworld_MC_Biomes"
    ],
    "fabric-datagen": [
      "net.brickcraftdream.rainworldmc_biomes.DataGenerator"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
    "sodium": ">=0.6.11"
  },
  "accessWidener" : "rainworldmc_biomes.accessWidener",
  "mixins": [
    "rainworld.mixins.json"
  ]
}
